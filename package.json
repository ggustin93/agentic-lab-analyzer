{
  "name": "health-document-analyzer",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "test": "ng test",
    "test:docker": "ng test --no-watch --browsers=ChromeHeadlessDocker --karma-config=karma.conf.js",
    "test:coverage": "ng test --no-watch --browsers=ChromeHeadlessDocker --karma-config=karma.conf.js --code-coverage",
    "e2e": "cypress run",
    "e2e:docker": "docker-compose run --rm cypress",
    "e2e:open": "cypress open",
    "test:all:docker": "npm run test:docker && docker-compose run --rm cypress",
    "lint": "ng lint",
    "dev:start": "./scripts/dev-start.sh",
    "dev:validate": "./scripts/validate-setup.sh",
    "dev:check-deps": "node scripts/check-deps.js",
    "dev:fix-deps": "./scripts/fix-dependencies.sh",
    "docker:clean": "./scripts/docker-cleanup.sh",
    "docker:monitor": "./scripts/docker-monitor.sh",
    "docker:test": "./scripts/test-docker.sh"
  },
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/katex": "^0.16.7",
    "katex": "^0.16.22",
    "marked": "^15.0.12",
    "ng2-pdf-viewer": "^10.3.1",
    "ngx-markdown": "19.0.0",
    "rxjs": "~7.8.0",
    "tippy.js": "^6.3.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@cypress/schematic": "^3.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/tippy.js": "^6.3.0",
    "angular-eslint": "20.1.1",
    "autoprefixer": "^10.4.19",
    "cypress": "^14.5.1",
    "eslint": "^9.29.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "8.34.1",
    "wait-on": "^8.0.3"
  }
}